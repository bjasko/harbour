# Merge duplicate strings
BCC_OPT = $(BCC_OPT) -d

HARBOUR_OPT = $(HARBOUR_OPT) -q0 -w

!if $d(B16)

# Borland C/C++ (DOS 16 bits)
BCC_EXE = bcc
BCC_OPT = $(BCC_OPT) -DHARBOUR_USE_DOS_GTAPI -mh
TASM_EXE = tasm
OBJ_DIR = obj\b16
LIB_DIR = lib\b16
BCC_OPT = $(BCC_OPT) -O2

!else

# Borland C/C++ (Windows 32 bits)
BCC_EXE = bcc32
BCC_OPT = $(BCC_OPT) -DHARBOUR_USE_WIN_GTAPI
TASM_EXE = tasm32
OBJ_DIR = obj\b32
LIB_DIR = lib\b32

!if !$d(B40)
BCC_OPT = $(BCC_OPT) -O2
!endif

!endif

