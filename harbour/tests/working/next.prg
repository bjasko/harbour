Function Main()
Local nExt, nExt7, nExtNEXT

    For NExt := 1 To 10

       OutStd( nExT ) // Actually this needs to use str()

   Next /*next*/ nExt 	//next
//NEXT

   nExt := 10 //next

   nExt++	/*n_ext*/	//---
   --nExt	/*next*/
   nExt7 :=7
   nExtNEXT := nExt	//next

   nExt[ nExt ] := NEXT( nExt[nExt+nExt] * nExt *(nExt+nExt*5) )

Return( nExt * /*next*/ nExt )

FUNCTION NeXT( nExt /*next next*/ )
  IF( nExt > nExt+4 )
    nExt =nExt +nExt * 5
  ELSEIF( nExt > 0 )
    nExt = nExt * 6 + nExt
  ELSE
    nExt +=5
  ENDIF
RETURN nExt * nExt
