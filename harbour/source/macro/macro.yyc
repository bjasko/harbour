yydestruct (yymsg, yytype, yyvaluep, pMacro)
    const char *yymsg;
    int yytype;
    YYSTYPE *yyvaluep;
    HB_MACRO_PTR pMacro;
#endif
{
  YYUSE (yyvaluep);
  YYUSE (pMacro);

  if (!yymsg)
    yymsg = "Deleting";
  YY_SYMBOL_PRINT (yymsg, yytype, yyvaluep, yylocationp);

  switch (yytype)
    {

      default:
