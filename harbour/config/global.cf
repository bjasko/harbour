all : first

GRANDP = ../../
ARCH = $(HB_ARCHITECTURE)/$(HB_COMPILER)

ARCH_DIR = $(ARCH)/

include $(TOP)$(ROOT)config/$(ARCH).cf

dirbase::
	@if [ ! -d $(HB_ARCH) ]; then \
	  $(MD) $(HB_ARCH); \
	fi; \
	if [ ! -d $(ARCH_DIR) ]; then \
	  $(MD) $(ARCH_DIR); \
	fi

clean::
	$(RM) -r $(ARCH_DIR)
